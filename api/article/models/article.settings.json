{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "tags": {
      "via": "articles",
      "collection": "tag",
      "dominant": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "articles"
    },
    "content": {
      "type": "richtext"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
